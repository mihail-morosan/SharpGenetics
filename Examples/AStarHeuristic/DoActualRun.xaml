<Page x:Class="SGGUI.DoActualRun"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SGGUI"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="559"
      Title="DoActualRun">

    <Grid Background="White">
        <TextBox x:Name="generationsToRun" Height="23" Margin="89,12,90,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top"/>
        <Button x:Name="button1" Content="Run" Margin="0,15,10,0" VerticalAlignment="Top" Click="button1_Click" HorizontalAlignment="Right" Width="75"/>
        <ListBox x:Name="listView1" Margin="10,40,0,10" HorizontalAlignment="Left" Width="243" MouseDoubleClick="listView1_MouseDoubleClick"/>
        <Label x:Name="label" Content="Generations" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="simulationView" Margin="258,40,10,10"/>

    </Grid>
</Page>
